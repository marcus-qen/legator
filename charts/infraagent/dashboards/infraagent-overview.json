{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Agent Fleet Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "Total Runs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(infraagent_runs_total)",
          "legendFormat": "Total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      }
    },
    {
      "title": "Success Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "targets": [
        {
          "expr": "sum(infraagent_runs_total{status=\"Succeeded\"}) / sum(infraagent_runs_total) * 100",
          "legendFormat": "Success %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 90 },
              { "color": "green", "value": 98 }
            ]
          }
        }
      }
    },
    {
      "title": "Active Runs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "infraagent_active_runs",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      }
    },
    {
      "title": "Total Tokens Used",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(infraagent_tokens_used_total)",
          "legendFormat": "Tokens",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null }
            ]
          }
        }
      }
    },
    {
      "title": "Guardrail Blocks",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "sum(infraagent_guardrail_blocks_total)",
          "legendFormat": "Blocks",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      }
    },
    {
      "title": "Escalations",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "expr": "sum(infraagent_escalations_total)",
          "legendFormat": "Escalations",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "title": "Runs by Agent",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "targets": [
        {
          "expr": "sum by (agent) (rate(infraagent_runs_total[5m]))",
          "legendFormat": "{{ agent }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "stacking": { "mode": "normal" }
          }
        }
      }
    },
    {
      "title": "Run Duration (p50/p95/p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le, agent) (rate(infraagent_run_duration_seconds_bucket[15m])))",
          "legendFormat": "{{ agent }} p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le, agent) (rate(infraagent_run_duration_seconds_bucket[15m])))",
          "legendFormat": "{{ agent }} p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, agent) (rate(infraagent_run_duration_seconds_bucket[15m])))",
          "legendFormat": "{{ agent }} p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2
          }
        }
      }
    },
    {
      "title": "Per-Agent Detail",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "title": "Runs by Status",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "sum by (status) (infraagent_runs_total)",
          "legendFormat": "{{ status }}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "right" }
      }
    },
    {
      "title": "Token Usage by Agent",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 14 },
      "targets": [
        {
          "expr": "sum by (agent) (rate(infraagent_tokens_used_total[15m]))",
          "legendFormat": "{{ agent }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "title": "Token Usage by Model",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 14 },
      "targets": [
        {
          "expr": "sum by (model) (infraagent_tokens_used_total)",
          "legendFormat": "{{ model }}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "right" }
      }
    },
    {
      "title": "Guardrails & Safety",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false
    },
    {
      "title": "Guardrail Blocks by Agent",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "targets": [
        {
          "expr": "sum by (agent) (rate(infraagent_guardrail_blocks_total[15m]))",
          "legendFormat": "{{ agent }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80
          },
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "title": "Findings by Severity",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "targets": [
        {
          "expr": "sum by (severity) (rate(infraagent_findings_total[15m]))",
          "legendFormat": "{{ severity }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "critical" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "warning" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "info" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "Scheduling",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "collapsed": false
    },
    {
      "title": "Schedule Lag by Agent",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
      "targets": [
        {
          "expr": "infraagent_schedule_lag_seconds",
          "legendFormat": "{{ agent }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 30 },
              { "color": "red", "value": 120 }
            ]
          }
        }
      }
    },
    {
      "title": "Iterations per Run",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "targets": [
        {
          "expr": "sum by (agent) (rate(infraagent_iterations_total[15m])) / sum by (agent) (rate(infraagent_runs_total[15m]))",
          "legendFormat": "{{ agent }} avg iterations/run",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10
          }
        }
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["infraagent", "kubernetes", "ai"],
  "templating": {
    "list": [
      {
        "current": {},
        "datasource": "${DS_PROMETHEUS}",
        "definition": "label_values(infraagent_runs_total, agent)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "agent",
        "query": "label_values(infraagent_runs_total, agent)",
        "refresh": 2,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "InfraAgent Overview",
  "uid": "infraagent-overview"
}
